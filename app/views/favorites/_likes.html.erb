<h1>Favorites#_likes</h1>
<p>Find me in app/views/users/index.html.erb</p>


  <ul class="media-list">
    <% users.each do |user| %>
    
      <li class="media">
        
        <div class="media-left">
          <img class="media-object img-rounded" src="<%= gravatar_url(user, { size: 50 }) %>" alt="">
        </div>
        
        
        <div class="media-body">
          
          <div>
            <%= user.name %>
          </div>

          
        </div>
        
      </li>
    
  </ul>
  
  <%= paginate users %>
  
<% end %>