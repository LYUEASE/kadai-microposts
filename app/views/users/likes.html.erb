<ul class="media-list">
  
  <% @likings.each do |like| %>
  
    <li class="media">
      
      
      <div class="media-body">
        
        <div>
          <%= link_to like.user.name, user_path(like.user) %> 
          <span class="text-muted">posted at <%= like.created_at %></span>
        </div>
        
        <div>
          <p><%= like.content %></p>
        </div>
        
      </div>
      
    </li>
    
  <% end %>
  
</ul>

    
